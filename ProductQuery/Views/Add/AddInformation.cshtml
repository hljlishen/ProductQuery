        function ModuleIsNone() {
            document.getElementById("form1").style.display = "none";
            document.getElementById("form2").style.display = "none";
            document.getElementById("form3").style.display = "none";
            document.getElementById("form4").style.display = "none";
            document.getElementById("form5").style.display = "none";
            document.getElementById("form6").style.display = "none";
            document.getElementById("form7").style.display = "none";
            document.getElementById("form8").style.display = "none";
            document.getElementById("form9").style.display = "none";
            document.getElementById("form10").style.display = "none";
        }

        function PageInformation(obj) {
            if (obj == "主页信息") {
                ModuleIsNone();
                document.getElementById("form1").style.display = "";
            }
            else if (obj == "添加信息模块") {
                ModuleIsNone();
                document.getElementById("form2").style.display = "";
            }
            else if (obj == "产品示意图") {
                ModuleIsNone();
                document.getElementById("form3").style.display = "";
            }
            else if (obj == "常规") {
                ModuleIsNone();
                document.getElementById("form4").style.display = "";
            }
            else if (obj == "索普通直径") {
                ModuleIsNone();
                document.getElementById("form5").style.display = "";
            }
            else if (obj == "爆速") {
                ModuleIsNone();
                document.getElementById("form6").style.display = "";
            }
            else if (obj == "接口信息") {
                ModuleIsNone();
                document.getElementById("form7").style.display = "";
            }
            else if (obj == "直流电阻") {
                ModuleIsNone();
                document.getElementById("form8").style.display = "";
            }
            else if (obj == "发火条件") {
                ModuleIsNone();
                document.getElementById("form10").style.display = "";
            }
            else if (obj == "延期时间") {
                ModuleIsNone();
                document.getElementById("form9").style.display = "";
            }
        }

        function AddInformationModule(obj) {
            var tables = $("table#test");
            var addtr = '<tr id="'+id+'">'+
                '<td class="tm-product-name">'+
                    '<ul class="navbar-nav">'+
                        '<li class="nav-item">'+
                            '<a class="nav-link d-block" href="#" name="'+obj+'" onclick="PageInformation(this.name)">'+obj+'</a>'+
                        '</li>'+
                    '</ul>'+
                '</td>'+
                '<td class="text-center">'+
                    '<a href="#" id="'+id+'" class="tm-product-delete-link" onclick="delAchieve(this.id)">'+
                        '<i class="far fa-trash-alt tm-product-delete-icon"></i>'+
                    '</a>'+
                '</td>'+
                '</tr>';
            id++;
            $(addtr).appendTo(tables);     
        }

        function delAchieve(obj) {
            $("table#test #"+obj+"").remove();
        }
    </script>
