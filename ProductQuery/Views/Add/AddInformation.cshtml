            if (obj == "接口信息")
                return jiekoxinxi;
            if (obj == "直流电阻")
                return zhiliudianzu;
            if (obj == "发火条件")
                return fahuotiaojian;
            if (obj == "延期时间")
                return yanqishijian;
        }

        function AddInformationModule(obj) {
            if (obj == "产品示意图" && ModuleNumber(obj) > 10) {
                alert("无法添加图片");
                return;
            }
            var tables = $("table#test");
            var addtr = '<tr id="' + id + '">' +
                '<td class="tm-product-name">' +
                '<ul class="navbar-nav">' +
                '<li class="nav-item">' +
