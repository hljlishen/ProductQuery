        function OpenFile() {
            document.getElementById("file").click();
        }

        $(function () {
            $("#file").on("change", function () {
                var file = this.files[0];
                if (this.files && file) {
                    var reader = new FileReader();
                    reader.onload = function (e) {
                        $('#img').attr('src', e.target.result);
                        $('#fn').text(file.name);
                    }
                    reader.readAsDataURL(file);
                }
            });
        });

