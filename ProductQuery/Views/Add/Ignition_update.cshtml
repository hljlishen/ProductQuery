        function FormSubmission() {
            var yzrq = true;
            var sjdxrq = true;
            var scdxrq = true;
            if (document.getElementById("expire_date").value != "") {
                var str = document.getElementById("expire_date").value.trim();
                if (str.length != 0) {
                    var reg = /^(\d{1,4})(-|\/)(\d{1,2})\2(\d{1,2})/;
                    yzrq = str.match(reg);
                    if (yzrq == null)
                        yzrq = false;
                }
            }
            if (document.getElementById("expire_date2").value != "") {
                var str = document.getElementById("expire_date2").value.trim();
                if (str.length != 0) {
                    var reg = /^(\d{1,4})(-|\/)(\d{1,2})\2(\d{1,2})/;
                    sjdxrq = str.match(reg);
                    if (sjdxrq == null)
                        sjdxrq = false;
                }
            }
            if (document.getElementById("expire_date3").value != "") {
                var str = document.getElementById("expire_date3").value.trim();
                if (str.length != 0) {
                    var reg = /^(\d{1,4})(-|\/)(\d{1,2})\2(\d{1,2})/;
                    scdxrq = str.match(reg);
                    if (scdxrq == null)
                        scdxrq = false;
                }
            }
            if (document.getElementById("类别").value == "请选择" || document.getElementById("产品名称").value == "" || yzrq == false || sjdxrq == false || scdxrq == false) {
                if (document.getElementById("类别").value == "请选择")
                    document.getElementById("lb").style.display = "";
                else
                    document.getElementById("lb").style.display = "none";
                if (document.getElementById("产品名称").value == "")
                    document.getElementById("cpmc").style.display = "";
                else
                    document.getElementById("cpmc").style.display = "none";
                if (yzrq == false)
                    document.getElementById("yzrq").style.display = "";
                else
                    document.getElementById("yzrq").style.display = "none";
                if (sjdxrq == false)
                    document.getElementById("sjdxrq").style.display = "";
                else
                    document.getElementById("sjdxrq").style.display = "none";
                if (scdxrq == false)
                    document.getElementById("scdxrq").style.display = "";
                else
                    document.getElementById("scdxrq").style.display = "none";
                alert("信息输入有误！");
                return;
            }
            document.getElementById("form").submit();
            alert("添加成功");
            parent.openFrame("Information");
        }

        $(document).ready(function () {
            //敲击按键时触发
            $("#double").bind("keypress", function (event) {
                var event = event || window.event;
                var getValue = $(this).val();
                //控制第一个不能输入小数点"."
                if (getValue.length == 0 && event.which == 46) {
                    event.preventDefault();
                    return;
                }
                //控制只能输入一个小数点"."
                if (getValue.indexOf('.') != -1 && event.which == 46) {
                    event.preventDefault();
                    return;
                }
                //控制只能输入的值
                if ((event.which < 48 || event.which > 57) && event.which != 8 && event.which != 46) {
                    event.preventDefault();
                    return;
                }
            })
        });
