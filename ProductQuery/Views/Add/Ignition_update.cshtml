        function PageInformation(obj) {
            if (obj == "添加信息模块") {
                document.getElementById("form").style.display = "none";
                document.getElementById("form2").style.display = "";
            }
            else {
                document.getElementById("form").style.display = "";
                document.getElementById("form2").style.display = "none";
            }
        }

        function ModuleNumber(obj) {
            if (obj == "产品示意图")
                return image;
            if (obj == "常规")
                return changgui;
            if (obj == "索普通直径")
                return suo;
            if (obj == "爆速")
                return baosu;
            if (obj == "接口信息")
                return jiekoxinxi;
            if (obj == "直流电阻")
                return zhiliudianzu;
            if (obj == "发火条件")
                return fahuotiaojian;
            if (obj == "延期时间")
                return yanqishijian;
        }

        function AddInformationModule(obj) {
            if (obj == "产品示意图" && ModuleNumber(obj) > 10) {
                alert("无法添加图片");
                return;
            }
            var tables = $("table#test");
            var addtr = '<tr id="' + id + '">' +
                '<td class="tm-product-name">' +
                '<ul class="navbar-nav">' +
                '<li class="nav-item">' +
                '<a class="nav-link d-block" href="#' + obj + ModuleNumber(obj) + '" onclick="PageInformation(this.name)">' + obj + ModuleNumber(obj) + '</a>' +
                '</li>' +
                '</ul>' +
                '</td>' +
                '<td class="text-center">' +
                '<a href="#" id="' + id + '" name="' + obj + '" class="tm-product-delete-link" onclick="delAchieve(this.id,this.name,' + ModuleNumber(obj) + ')">' +
                '<i class="far fa-trash-alt tm-product-delete-icon"></i>' +
                '</a>' +
                '</td>' +
                '</tr>';

            $(addtr).appendTo(tables);
            GenerateTemplate(obj);
            id++;
        }

        function GenerateTemplate(obj) {
            if (obj == "常规") {
                常规(obj);
                changgui++;
            }
            if (obj == "产品示意图") {
                document.getElementById(obj+image).style.display = "";
                image++;
            }
            if (obj == "索普通直径") {
                索普通直径(obj);
                suo++;
            }
            if (obj == "爆速") {
                爆速(obj);
                baosu++;
            }
            if (obj == "接口信息") {
                接口信息(obj);
                jiekoxinxi++;
            }
            if (obj == "直流电阻") {
                直流电阻(obj);
                zhiliudianzu++;
            }
            if (obj == "发火条件") {
                发火条件(obj);
                fahuotiaojian++;
            }
            if (obj == "延期时间") {
                延期时间(obj);
                yanqishijian++;
            }
        }



        function clearNoNum(obj) {
            obj.value = obj.value.replace(/[^\d.]/g, ""); //清除"数字"和"."以外的字符
            obj.value = obj.value.replace(/^\./g, ""); //验证第一个字符是数字而不是
            obj.value = obj.value.replace(/\.{2,}/g, "."); //只保留第一个. 清除多余的
            obj.value = obj.value.replace(".", "$#$").replace(/\./g, "").replace("$#$", ".");
        }

        function OpenFile(obj) {
            document.getElementById(obj).click();
        }

        $(function () {
            $("#file_1").on("change", function () {
                var file = this.files[0];
                if (file != null) {
                    if (!/image\/\w+/.test(file.type)) {
                        alert('您选择的不是图片，请上传一个图片');
                        return false;
                    }
                }
                var fileSize = file.size / 1024;
                if (fileSize > 5000) {
                    alert("您选择的图片太大，请选择小于5M的图片");
                    return;
                }
                if (this.files && file) {
                    var reader = new FileReader();
                    reader.onload = function (e) {
                        $('#img_1').attr('src', e.target.result);
                        $('#fn_1').text(file.name);
                        $('#cp_1').attr('value', e.target.result);
                    }
                    reader.readAsDataURL(file);
                }
            });
        });
